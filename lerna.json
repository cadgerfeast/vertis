{
  "$schema": "./node_modules/@lerna-lite/cli/schemas/lerna-schema.json",
  "version": "0.4.0",
  "command": {
    "version": {
      "allowBranch": [
        "master"
      ],
      "conventionalCommits": true,
      "message": "chore: release",
      "tagVersionPrefix": "vertis@",
      "changelog": false,
      "private": false,
      "yes": true
    },
    "publish": {
      "private": false,
      "yes": true,
      "preid": "alpha",
      "preDistTag": "alpha"
    }
  }
}
